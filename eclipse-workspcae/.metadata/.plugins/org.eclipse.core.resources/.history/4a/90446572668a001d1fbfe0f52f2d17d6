package org.opentutorials.javatutorials.variables;

public class StringDemo {

	public static void main(String[] args) {
		/*
		 * String a, b, c; a = "박남규의"; b = "인스타 아이디는"; c = "gunami 입니다.";
		 */
		
		String a = "박남규의";
		String b = "인스타 아이디는 ";
		String c = "belugacurt 입니다";

		System.out.println(a + b + c);
	}
}